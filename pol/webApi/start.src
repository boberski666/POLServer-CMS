use os;

Print("Initializing web API.");

var script := Start_Script("export/charExport");
if ( script.errortext )
	SysLog("Error starting char export ->"+script.errortext);
else
	Print("Starting char export... OK.");
endif

script := Start_Script("export/onlineExport");
if ( script.errortext )
	SysLog("Error starting online export ->"+script.errortext);
else
	Print("Starting online export... OK.");
endif
